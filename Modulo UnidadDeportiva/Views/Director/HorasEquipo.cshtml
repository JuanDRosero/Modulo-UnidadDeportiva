@model IEnumerable<EquipoModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<!-- Pasar a .net -->
<div id="layoutSidenav_content">
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            Horas de equipo
        </div>
        <div class="card-body">
            <table class="table">
                <thead align="center">
                    <tr>
                        <th>@Html.DisplayNameFor(x=>x.NombreEquipo)</th>
                        <th>@Html.DisplayNameFor(x=>x.Miembro)</th>
                        <th>@Html.DisplayNameFor(x=>x.Periodo)</th>
                        <th>@Html.DisplayNameFor(x=>x.Sede)</th>
                        <th>@Html.DisplayNameFor(x=>x.FechaHoraRegistro)</th>
                    </tr>
                </thead>
                <tbody align="center">
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(x=>item.NombreEquipo)</td>
                        <td>@Html.DisplayFor(x=>item.Miembro)</td>
                        <td>@Html.DisplayFor(x=>item.Periodo)</td>
                        <td>@Html.DisplayFor(x=>item.Sede)</td>
                        <td>@Html.DisplayFor(x=>item.FechaHoraRegistro)</td>
                    </tr>
                }
                </tbody>
            </table>
            <button class="btn btn-primary" id="btnPrint" onclick="imprimir()">Generar pdf</button>
        </div>
    </div>
</div>
<script type="text/javascript">
    function imprimir(){
        window.print();
    }
</script>
