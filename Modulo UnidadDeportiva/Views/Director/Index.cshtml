@model Empleado
@{
    ViewData["Title"] = "Director Page";
}

<!--Aca mismo hacemos que genere los reportes?-->
<div class="text-center">
    <h2 class="display-4">@Html.DisplayFor(x=>Model.NombreEmpleado)</h2>
